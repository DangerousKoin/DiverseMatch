ably a duplicate username\n    throw new Error('Username already taken!');\n  })\n  // Parameter destructuring!\n  .then(({token}) => tokenService.setToken(token));\n  // Setting our token in localStorage in our browser\n  // then we'll be able to use with every request!\n  // The above could have been written as\n  //.then((token) => token.token);\n}\n\nfunction getUser() {\n  return tokenService.getUserFromToken();\n}\n\nfunction logout() {\n  tokenService.removeToken();\n}\n\nfunction login(creds) {\n  return fetch(BASE_URL + 'login', {\n    method: 'POST',\n    headers: new Headers({'Content-Type': 'application/json'}),\n    body: JSON.stringify(creds)\n  })\n  .then(res => {\n    // Valid login if we have a status of 2xx (res.ok)\n    if (res.ok) return res.json();\n    throw new Error('Bad Credentials!');\n  })\n  .then(({token}) => tokenService.setToken(token));\n}\n\nfunction getProfile(username){\n  return fetch(BASE_URL + username, {\n    headers: {\n      'Authorization': 'Bearer ' + tokenService.getToken()\n    }\n  }).then(res => {\n    if(res.ok) return res.json()\n    throw new Error('User does not exist') // This is the error in our try catch(err)\n  })\n}\n\nexport default {\n  signup, \n  getUser,\n  logout,\n  login,\n  getProfile\n};","/home/koin/code/DiverseMatch/src/utils/tokenService.js",["148"],"function setToken(token) {\n    if (token) {\n      // localStorage is given to us by the browser\n      localStorage.setItem('token', token);\n    } else {\n      localStorage.removeItem('token');\n    }\n  }\n  \n  function getToken() {\n    let token = localStorage.getItem('token');\n    if (token) {\n      // Check if expired, remove if it is\n      // atob is a function that decodes a base-64 string\n      const payload = JSON.parse(atob(token.split('.')[1]));\n      // JWT's exp is expressed in seconds, not milliseconds, so convert\n      if (payload.exp < Date.now() / 1000) {\n        localStorage.removeItem('token');\n        token = null;\n      }\n    }\n    return token;\n  }\n  \n  function getUserFromToken() {\n    const token = getToken();\n    return token ? JSON.parse(atob(token.split('.')[1])).user : null;\n  }\n  \n  function removeToken() {\n    localStorage.removeItem('token');\n  }\n  \n  export default {\n    setToken,\n    getToken,\n    removeToken,\n    getUserFromToken\n  };","/home/koin/code/DiverseMatch/src/utils/topicService.js",[],"/home/koin/code/DiverseMatch/src/pages/App.jsx",["149","150"],"/home/koin/code/DiverseMatch/src/layouts/Feed.jsx",["151","152","153","154","155","156"],"/home/koin/code/DiverseMatch/src/components/AddTopicForm.jsx",[],"/home/koin/code/DiverseMatch/src/components/ProfileBio.jsx",[],"/home/koin/code/DiverseMatch/src/components/Search.jsx",["157","158","159","160"],"/home/koin/code/DiverseMatch/src/components/TopicCard.js",["161","162"],"/home/koin/code/DiverseMatch/src/components/ErrorMessage.jsx",[],"/home/koin/code/DiverseMatch/src/layouts/ProfilePage.jsx",["163","164"],"/home/koin/code/DiverseMatch/src/components/TopicFeed.jsx",[],"/home/koin/code/DiverseMatch/src/components/Sidebar.jsx",["165"],{"ruleId":"166","severity":1,"message":"167","line":56,"column":1,"nodeType":"168","endLine":62,"endColumn":3},{"ruleId":"169","severity":1,"message":"170","line":9,"column":23,"nodeType":"171","messageId":"172","endLine":9,"endColumn":35},{"ruleId":"169","severity":1,"message":"170","line":9,"column":25,"nodeType":"171","messageId":"172","endLine":9,"endColumn":37},{"ruleId":"169","severity":1,"message":"173","line":2,"column":16,"nodeType":"171","messageId":"172","endLine":2,"endColumn":23},{"ruleId":"169","severity":1,"message":"174","line":2,"column":25,"nodeType":"171","messageId":"172","endLine":2,"endColumn":31},{"ruleId":"169","severity":1,"message":"175","line":15,"column":12,"nodeType":"171","messageId":"172","endLine":15,"endColumn":17},{"ruleId":"169","severity":1,"message":"176","line":52,"column":19,"nodeType":"171","messageId":"172","endLine":52,"e