ge in our browser\n  // then we'll be able to use with every request!\n  // The above could have been written as\n  //.then((token) => token.token);\n}\n\nfunction getUser() {\n  return tokenService.getUserFromToken();\n}\n\nfunction logout() {\n  tokenService.removeToken();\n}\n\nfunction login(creds) {\n  return fetch(BASE_URL + 'login', {\n    method: 'POST',\n    headers: new Headers({'Content-Type': 'application/json'}),\n    body: JSON.stringify(creds)\n  })\n  .then(res => {\n    // Valid login if we have a status of 2xx (res.ok)\n    if (res.ok) return res.json();\n    throw new Error('Bad Credentials!');\n  })\n  .then(({token}) => tokenService.setToken(token));\n}\n\nfunction getProfile(username){\n  return fetch(BASE_URL + username, {\n    headers: {\n      'Authorization': 'Bearer ' + tokenService.getToken()\n    }\n  }).then(res => {\n    if(res.ok) return res.json()\n    throw new Error('User does not exist') // This is the error in our try catch(err)\n  })\n}\n\nexport default {\n  signup, \n  getUser,\n  logout,\n  login,\n  getProfile\n};","/home/koin/code/DiverseMatch/src/utils/tokenService.js",["144"],"function setToken(token) {\n    if (token) {\n      // localStorage is given to us by the browser\n      localStorage.setItem('token', token);\n    } else {\n      localStorage.removeItem('token');\n    }\n  }\n  \n  function getToken() {\n    let token = localStorage.getItem('token');\n    if (token) {\n      // Check if expired, remove if it is\n      // atob is a function that decodes a base-64 string\n      const payload = JSON.parse(atob(token.split('.')[1]));\n      // JWT's exp is expressed in seconds, not milliseconds, so convert\n      if (payload.exp < Date.now() / 1000) {\n        localStorage.removeItem('token');\n        token = null;\n      }\n    }\n    return token;\n  }\n  \n  function getUserFromToken() {\n    const token = getToken();\n    return token ? JSON.parse(atob(token.split('.')[1])).user : null;\n  }\n  \n  function removeToken() {\n    localStorage.removeItem('token');\n  }\n  \n  export default {\n    setToken,\n    getToken,\n    removeToken,\n    getUserFromToken\n  };","/home/koin/code/DiverseMatch/src/utils/topicService.js",[],"/home/koin/code/DiverseMatch/src/pages/App.jsx",["145","146","147"],"/home/koin/code/DiverseMatch/src/layouts/Content.jsx",["148","149"],"/home/koin/code/DiverseMatch/src/layouts/Sidebar.jsx",["150"],"/home/koin/code/DiverseMatch/src/components/Search/TopicSearch.jsx",[],"/home/koin/code/DiverseMatch/src/components/Cards/TopicCard.jsx",["151","152"],"/home/koin/code/DiverseMatch/src/components/Displays/ProfileDisplay.jsx",[],"/home/koin/code/DiverseMatch/src/components/Forms/SignupForm.jsx",["153","154"],"/home/koin/code/DiverseMatch/src/components/System/ErrorMessage.jsx",[],"/home/koin/code/DiverseMatch/src/components/Forms/LoginForm.jsx",["155"],{"ruleId":"156","severity":1,"message":"157","line":56,"column":1,"nodeType":"158","endLine":62,"endColumn":3},{"ruleId":"159","severity":1,"message":"160","line":9,"column":23,"nodeType":"161","messageId":"162","endLine":9,"endColumn":35},{"ruleId":"159","severity":1,"message":"160","line":9,"column":25,"nodeType":"161","messageId":"162","endLine":9,"endColumn":37},{"ruleId":"159","severity":1,"message":"163","line":2,"column":16,"nodeType":"161","messageId":"162","endLine":2,"endColumn":23},{"ruleId":"159","severity":1,"message":"164","line":2,"column":25,"nodeType":"161","messageId":"162","endLine":2,"endColumn":31},{"ruleId":"159","severity":1,"message":"165","line":15,"column":12,"nodeType":"161","messageId":"162","endLine":15,"endColumn":17},{"ruleId":"159","severity":1,"message":"166","line":52,"column":19,"nodeType":"161","messageId":"162","endLine":52,"endColumn":23},{"ruleId":"167","severity":1,"message":"168","line":62,"column":8,"nodeType":"169","endLine":62,"endColumn":10,"suggestions":"170"},{"ruleId":"159","severity":1,"message":"166","line":52,"column":17,"nodeType":"161","messageId":"162","en